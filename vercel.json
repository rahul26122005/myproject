{
    "version": 2,
    "builds": [
      {
        "src": "dj_project/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "env" : {
          "MY_ENV_VAR": "C:/Users/rahul/.virtualenvs/django-B5393Xc0"
          
        },
        "maxLambdaSize": "15mb", "runtime": "python3.12"
        
      }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "dj_project/wsgi.py"
      }
    ]
  }